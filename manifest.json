{
  "manifest_version": 3,
  "name": "한국학 공구서",
  "version": "1.1",
  "description": "동양연표 + 한국학 웹 사전(한어대사전, 이두, 한의학 용어 등) + 한자입력기",
  "permissions": ["sidePanel"],
  "side_panel": {
    "default_path": "sidepanel.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "action": {
    "default_title": "한국학 공구서 열기"
  }
}
